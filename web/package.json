{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,vue}\"",
    "prepare": "cd .. && husky web/.husky"
  },
  "dependencies": {
    "@vuestic/tailwind": "^0.1.5",
    "axios": "^1.11.0",
    "jwt-decode": "^4.0.0",
    "pdfmake": "^0.2.20",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuestic-ui": "^1.10.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.12.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "9.35.0",
    "eslint-plugin-vue": "10.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "material-design-icons-iconfont": "^6.7.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "start-server-and-test": "^2.0.13",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.9.2",
    "vite": "^7.1.0",
    "vitest": "^3.2.4",
    "vue-i18n": "^11.1.11",
    "vue-tsc": "^3.0.5"
  }
}
